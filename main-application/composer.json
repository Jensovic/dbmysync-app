{
    "name": "jensovic/dbmysync",
    "description": "Database synchronization tool - Compare and sync MySQL databases across environments",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Jensovic",
            "email": "jensovic@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.4",
        "ext-pdo": "*",
        "ext-pdo_sqlite": "*",
        "guzzlehttp/guzzle": "^7.0",
        "twig/twig": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Jensovic\\DbMySync\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Jensovic\\DbMySync\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}

